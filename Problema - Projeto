(define (problem IAI)
    (:domain logistics-typed)
    (:objects
        caixa - CAIXA
        macaco1 - MACACO
        macaco2 - MACACO
		A - LUGAR
		B - LUGAR
		D1 - LUGAR
		D2 - LUGAR
		D3 - LUGAR
		D4 - LUGAR
		banana1 - BANANA
		banana2 - BANANA
		banana3 - BANANA
		banana4 - BANANA
    )
    (:init
        (ocupada caixa)
	    (baixo macaco1)
	    (alto macaco2)
	    (alto banana1)
	    (alto banana2)
        (baixo banana3)
	    (baixo banana4)
        (at macaco1 A)
        (at macaco2 B)
		(at caixa B)
		(at banana1 B)
		(at banana2 D2)
		(at banana3 D3)
		(at banana4 D4)
		(vazio macaco1)
		(carregado macaco2 banana1)
    )
    (:goal (and
	(carregado macaco1  banana1)
	(carregado macaco2  banana1)
	(at macaco1 A)
	))
)

